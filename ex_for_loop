// ex_for_loop

#include <stdio.h>

#if 0 
void main(void){
	int i , j ; 
	
	for ( i=0 ; i<5 ; i++){
		for(j=0 ; j<5 ; j++ )
			printf("*");
		printf("\n");
	}
}

void main(void){
	int i , j ; 
	
	for ( i=0 ; i<=5 ; i++){
		for(j=0 ; i<j  ; j++ )
			printf("*");
		printf("\n");
	}
}


void main(void){
	int i , j ; 
	
	for ( i=0 ; i<=5 ; i++){
		for(j=5 ; j>i ; j-- )
			printf("*");
		printf("\n");
	}
}

void main(void){
	int i , j ; 
	
	for ( i=5 ; i>=0 ; i--){
		for(j=5 ; j>i ; j-- )
			printf("*");
		printf("\n");
	}
}
#endif
